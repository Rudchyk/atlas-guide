{{#nodes}}
    <div role="listitem" class="atlas-component-structure__node">
        <div class="atlas-component-structure__selector" title="{{type}}">
            <span class="atlas-component-structure__type atlas-component-structure__type_{{type}}"></span>
            <span class="atlas-component-structure__name">{{node}}</span>{{#isBlock}} {...}{{/isBlock}}{{^isBlock}};{{/isBlock}}
        </div>
        {{#nodes.length}}
            <div role="list" class="atlas-component-structure__nodes">
                {{>componentstructure}}
            </div>
        {{/nodes.length}}
    </div>
{{/nodes}}
